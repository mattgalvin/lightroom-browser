# Adobe OAuth2 Credentials
# Get these from https://developer.adobe.com/console
ADOBE_CLIENT_ID=your_client_id_here
ADOBE_CLIENT_SECRET=your_client_secret_here

# OAuth Redirect URI (must match the one configured in Adobe Console)
ADOBE_REDIRECT_URI=https://localhost:8443/callback

# Flask Configuration
FLASK_SECRET_KEY=your-secret-key-change-in-production
FLASK_DEBUG=True
PORT=8443

# Pagination Configuration
ALBUMS_PER_PAGE=8
PHOTOS_PER_PAGE=20

